      }
    },
    "core/heading/h2": {
      "typography": {
        "fontSize": "var(--wp--preset--font-size--large)"
      }
    },
    "core/heading/h3": {
      "typography": {
        "fontSize": "var(--wp--preset--font-size--medium)"
      }
    },
    "core/heading/h4": {
      "typography": {
        "fontSize": "var(--wp--preset--font-size--normal)"
      }
    },
    "core/heading/h5": {
      "typography": {
        "fontSize": "var(--wp--preset--font-size--small)"
      }
    },
    "core/heading/h6": {
      "typography": {
        "fontSize": "var(--wp--preset--font-size--small)"
      }
    }
  },
  "templateParts": {
    "header": {
      "area": "header"
